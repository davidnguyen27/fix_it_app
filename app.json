{
  "expo": {
    "name": "Fixit Right",
    "slug": "fixit-right",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/splash-icon-dark.png",
    "scheme": "fixitright",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#4A628A"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icons/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "resizeMode": "cover",
          "backgroundColor": "#FFFFFF",
          "image": "./assets/icons/splash-icon.png",
          "imageWidth": 200,
          "enableFullScreenImage_legacy": true
        }
      ],
      [
        "expo-font",
        {
          "font": [
            "./assets/fonts/Unbounded-Light.ttf",
            "./assets/fonts/Unbounded-Regular.ttf",
            "./assets/fonts/Unbounded-Medium.ttf",
            "./assets/fonts/Unbounded-SemiBold.ttf",
            "./assets/fonts/Unbounded-Bold.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6fde0d13-7aa1-4509-b52c-e9e19371c064"
      }
    }
  }
}
